{
    "name": "code-york/connect-four",
    "description": "A Connect Four Server Built In Lumen.",
    "type": "project",
    "authors": [
        {
            "name": "Graham Campbell",
            "email": "graham@alt-three.com"
        }
    ],
    "require": {
        "php": "^7.0.0",
        "laravel/lumen-framework": "~5.2.7",
        "alt-three/bugsnag": "^1.4",
        "alt-three/logger": "^1.1",
        "graham-campbell/exceptions": "^8.6",
        "illuminate/redis": "~5.2.0",
        "predis/predis": "^1.1",
        "vlucas/phpdotenv": "^2.2"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.5",
        "graham-campbell/testbench-core": "^1.1",
        "phpunit/phpunit": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "CodeYork\\ConnectFour\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CodeYork\\Tests\\ConnectFour\\": "tests/"
        }
    },
    "config": {
        "platform": {
            "php": "7.0.0"
        },
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
